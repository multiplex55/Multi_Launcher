{
  "version": 1,
  "grid": {
    "rows": 4,
    "cols": 3
  },
  "slots": [
    {
      "id": "Weather",
      "widget": "weather_site",
      "row": 0,
      "col": 0,
      "row_span": 1,
      "col_span": 2,
      "settings": {
        "location": "Seattle"
      }
    },
    {
      "id": "Recents",
      "widget": "recent_commands",
      "row": 1,
      "col": 0,
      "row_span": 1,
      "col_span": 1
    },
    {
      "id": "Frequently used",
      "widget": "frequent_commands",
      "row": 1,
      "col": 1,
      "row_span": 1,
      "col_span": 1
    },
    {
      "id": "Pinned results",
      "widget": "pinned_query_results",
      "row": 1,
      "col": 2,
      "row_span": 1,
      "col_span": 1,
      "settings": {
        "engine": "omni_search",
        "query": "o list",
        "limit": 6,
        "refresh_interval_secs": 45,
        "click_behavior": "run_action"
      }
    },
    {
      "id": "Todos",
      "widget": "todo",
      "row": 2,
      "col": 0,
      "row_span": 1,
      "col_span": 1
    },
    {
      "id": "Notes",
      "widget": "notes_open",
      "row": 2,
      "col": 1,
      "row_span": 1,
      "col_span": 1
    },
    {
      "id": "System Status",
      "widget": "system_status",
      "row": 2,
      "col": 2,
      "row_span": 1,
      "col_span": 1,
      "settings": {
        "refresh_interval_secs": 5,
        "show_network": true,
        "show_volume": true,
        "show_brightness": true
      }
    },
    {
      "id": "Now Playing",
      "widget": "now_playing",
      "row": 3,
      "col": 0,
      "row_span": 1,
      "col_span": 1
    },
    {
      "id": "Query List",
      "widget": "query_list",
      "row": 3,
      "col": 1,
      "row_span": 1,
      "col_span": 1,
      "settings": {
        "query": "o list",
        "count": 5,
        "refresh_ms": 3000,
        "show_desc": true
      }
    },
    {
      "id": "Quick Tools",
      "widget": "quick_tools",
      "row": 3,
      "col": 2,
      "row_span": 1,
      "col_span": 1,
      "settings": {
        "queries": [
          "sys",
          "net",
          "info cpu",
          "vol",
          "bright"
        ]
      }
    }
  ]
}
